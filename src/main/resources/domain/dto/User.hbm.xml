<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="domain.dto">
    <class name="UserDTO" table="user">
        <id name="id" type="java.lang.Long">
            <generator class="native" />
        </id>
        
        
        <property name="name" column="name"/>
        <property name="surname" column="surname"/>
        <property name="username" column="username"/>
        <property name="numberOfReviews" column="numberOfReviews"/>
        <property name="numberOfCheckins" column="numberOfCheckins"/>
        <property name="login" column="login"/>
        <property name="facebookID" column="facebookID"/>
        <property name="status" column="status"/>
        <property name="photoUrl" column="photoUrl"/>
        <property name="lastLoginDate" column="lastLoginDate"/>
        <property name="password" column="password"/>
        <property name="email" column="email"/>
        <property name="version" column="version"/>
        <property name="androidGcmToken" column="androidGcmToken"/>
    </class>
</hibernate-mapping>